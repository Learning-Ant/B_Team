<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.koreait.baraON.dao.memberDao2">

	<select id="memberSearch" parameterType="String" resultType="int">
		SELECT COUNT(*)
		FROM MEMBER
		WHERE MID=#{mId}
	</select>
	
	<delete id="memberDrop" parameterType="int">
		DELETE 
		FROM MEMBER
		WHERE NO =#{no}
	</delete>
	

</mapper>