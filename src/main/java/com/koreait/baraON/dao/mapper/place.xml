<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.koreait.baraON.dao.PlaceDao2">

	<select id="allPlaceSearch" parameterType="int" resultType="com.koreait.baraON.dto.PlaceDto2">
		SELECT *
		FROM PLACE
		WHERE S_NO={s_no}
	</select>
	
	<select id="placeCaSearch" parameterType="com.koreait.baraON.dto.PlaceDto2" resultType="com.koreait.baraON.dto.PlaceDto2">
		SELECT *
		FROM PLACE
		WHERE S_NO={s_no} AND PC_NO={pc_no}
	</select>

</mapper>